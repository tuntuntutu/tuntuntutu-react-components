{
  "name": "tuntuntutu-react-components",
  "version": "1.0.3",
  "main": "lib/index.js",
  "module": "es/index.js",
  "sideEffects": [
    "components/**/*.less"
  ],
  "files": [
    "es",
    "lib"
  ],
  "scripts": {
    "start": "tuntuntutu-scabbard serve --open",
    "dev": "tuntuntutu-scabbard serve --open",
    "precommit": "lint-staged",
    "test": "jest components/*/__test__/*.test.js --colors --config .jest.js",
    "wintest": "jest --colors --config .jest.js",
    "es": "cross-env NODE_ENV=production c3-tools run -t es",
    "lib": "cross-env NODE_ENV=production c3-tools run -t commonjs"
  },
  "license": "ISC",
  "babel": {
    "presets": [
      "tuntuntutu"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "eslintIgnore": [
    "build"
  ],
  "dependencies": {
    "antd": "^3.6.3",
    "axios": "0.18.0",
    "localforage": "1.7.2",
    "moment": "^2.22.2",
    "prop-types": "15.6.2",
    "qs": "6.5.2",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-image-lightbox-rotate": "1.2.0",
    "react-quill": "^1.3.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-tuntuntutu": "^1.0.1",
    "c3-tools": "^1.5.0-alpha.7",
    "cross-env": "^5.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-tuntuntutu": "^1.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.4.2",
    "koa-router": "^7.4.0",
    "koa2": "^2.0.0-alpha.7",
    "koa2-cors": "^2.0.6",
    "react-test-renderer": "^16.4.2",
    "regenerator-runtime": "^0.12.0",
    "sinon": "^6.1.4",
    "tuntuntutu-scabbard": "^1.0.1"
  }
}
